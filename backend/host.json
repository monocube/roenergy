{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api",
            "customHeaders": {
                "Cache-Control": "public, max-age=86400"
            }
        }
    }
}